{
  "name": "tz-parser",
  "version": "1.0.0",
  "description": "Parse raw data from TZ-AVL devices",
  "main": "lib",
  "scripts": {
    "prepublish": "npm run build -s",
    "prebuild": "npm run lint -s && npm run clean -s",
    "build": "babel src --out-dir lib --source-maps",
    "lint": "eslint src",
    "clean": "rimraf lib"
  },
  "engines": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/tz-parser.git"
  },
  "keywords": [
    "rut"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgaticaq)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/tz-parser/issues"
  },
  "homepage": "https://github.com/lgaticaq/tz-parser#readme",
  "dependencies": {
    "crc": "^3.4.0",
    "moment": "^2.11.2",
    "node-nmea": "^0.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^1.10.3",
    "rimraf": "^2.5.1"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "tonicExampleFilename": "example.js"
}
